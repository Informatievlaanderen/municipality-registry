{
  "DataDog": {
    "Enabled": true,
    "Debug": false,
    "ServiceName": "municipality-registry-projector-api"
  },

  "Cors": [
    "http://localhost:3000",
    "http://localhost:5000"
  ],

  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning",
      "MunicipalityRegistry": "Debug",
      "Aiv": "Debug",
      "Aiv.Vbr.DataDog.Tracing": "Information",
      "Microsoft": "Warning",
      "System": "Warning"
    }
  },

  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "MunicipalityRegistry": "Verbose",
        "Aiv": "Verbose",
        "Aiv.Vbr.DataDog.Tracing": "Information",
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Properties": {
      "Application": "MunicipalityRegistry - Projector API"
    }
  }
}
